{
  "breadcrumbs.enabled": true,
  "[cpp]": {
    "editor.autoIndent": "full"
  },
  "C_Cpp.clang_format_fallbackStyle": "{BasedOnStyle: Google, IndentWidth: 4, ContinuationIndentWidth: 8, ColumnLimit: 100, AccessModifierOffset: -3, PointerBindsToType: false, DerivePointerAlignment: false, ConstructorInitializerIndentWidth: 8, AllowShortFunctionsOnASingleLine: false, AllowShortLoopsOnASingleLine: false, AllowShortIfStatementsOnASingleLine: false}",
  "C_Cpp.clang_format_path": "/usr/bin/clang-format-6.0",
  "C_Cpp.default.includePath": [
      "${workspaceFolder}/**"
  ],
  "C_Cpp.default.intelliSenseMode": "clang-x64",
  "C_Cpp.intelliSenseEngine": "Tag Parser",
  "cpplint.cpplintPath": "/usr/local/bin/cpplint",
  "cpplint.lineLength": 100,
  "debug.toolBarLocation": "docked",
  "diffEditor.ignoreTrimWhitespace": false,
  "doxdocgen.cpp.tparamTemplate": "\\tparam {param} ",
  "doxdocgen.generic.briefTemplate": "{text}",
  "doxdocgen.generic.paramTemplate": "\\param {param} ",
  "doxdocgen.generic.returnTemplate": "\\return {type} ",
  "editor.detectIndentation": false,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.formatOnSave": false,
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "editor.renderControlCharacters": false,
  "editor.renderWhitespace": "all",
  "editor.rulers": [
